<div class= "row">
  <div class="page-heading-bar">
    <ol class="breadcrumb">
      <li >Admin</li>
      <li><%= link_to("All Events", {:action => 'index'}) %></li>
      <li >Show</li>
      <li class="active"><%= @event.event_name.capitalize %></li>
    </ol>
  </div>
</div>
<div class="col-md-6 col-md-offset-3">

	
	<table class="display table-striped responsive "summary="Event detail view">
		<tr>
			<th>Event ID</th>
			<td><%= @event.id %></td>
		</tr>
		<tr>
			<th>Name</th>
			<td><%= @event.event_name %></td>
		</tr>
		<tr>
			<th>Description</th>
			<td><%= @event.description.html_safe %> </td>
		</tr>
	</table>
	<table>
		<tr>
			<th>QR Code</th>
			<td><%= raw @qr.as_html %></td>
		</tr>
	</table>
</div>
</div>





