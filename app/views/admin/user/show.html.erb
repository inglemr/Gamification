<div id= "ribbon">
	<ol class="breadcrumb">
	 	<li >Admin</li>
	 	<li >Users</li>
	  <li><%= link_to("All Users", {:action => 'index'}) %></li>
	  <li class="active"><%= @user.username %></li>
	</ol>
</div>
<div class="col-md-6 col-md-offset-3">

	
	<table class="display table-striped responsive "summary="User detail view">
		<tr>
			<th>User ID</th>
			<td><%= @user.id %></td>
		</tr>
		<tr>
			<th>Email</th>
			<td><%= @user.email %></td>
		</tr>
		<tr>
			<th>Created at</th>
			<td><%= @user.created_at %> </td>
		</tr>
		<tr>
			<th>Sign in count</th>
			<td><%= @user.sign_in_count %></td>
		</tr>
		<tr>
			<th>Updated</th>
			<td><%= @user.updated_at %></td>
		</tr>
		<tr>
			<th>API Token</th>
			<td><%= @user.api_token %></td>
		</tr>
	</table>
</div>
</div>
